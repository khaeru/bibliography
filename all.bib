	keywords = {Emission factor, Diesel, Climate change, Air pollution, Nitrogen oxides, Black carbon},
	localfile = {Wang X 2012.pdf},
	month = {1},
	pages = {45–55},
	title = {{On-road diesel vehicle emission factors for nitrogen oxides and black carbon in two Chinese cities}},
