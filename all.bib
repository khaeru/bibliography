	journaltitle = {Applied Energy},
	keywords = {On-road vehicle, GHG emissions, Peak, Provincial analysis},
	localfile = {Zheng B 2015b.pdf},
	pages = {230–240},
	title = {{How will greenhouse gas emissions from motor vehicles be constrained in China around 2030?}},
	volume = {156},
	year = {2015}
}

